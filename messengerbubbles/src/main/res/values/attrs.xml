<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MessCloudView">
        <attr name="cloudColor" format="color"/>
        <attr name="arrowSize" format="dimension"/>
        <attr name="cloudMessage" format="string"/>
        <attr name="currentWall" format="enum" >
            <enum name="LEFT" value="0"/>
            <enum name="RIGHT" value="1"/>
        </attr>
        <attr name="cloudHorizontalPadding" format="dimension"/>
        <attr name="cloudVerticalPadding" format="dimension"/>
        <attr name="cloudMaxWidth" format="dimension"/>
        <attr name="cloudMessageColor" format="color"/>
    </declare-styleable>
</resources>